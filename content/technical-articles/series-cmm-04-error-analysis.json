          "label": "Type B Uncertainty",
          "value": "Other methods",
          "unit": "calibration certificates, specifications"
        },
        {
          "label": "Combined Uncertainty",
          "value": "uc = √(uA² + uB²)",
          "unit": "root sum of squares"
        },
        {
          "label": "Expanded Uncertainty",
          "value": "U = k × uc",
          "unit": "coverage factor k=2 (95% confidence)"
        },
        {
          "label": "Target Ratio",
          "value": "U ≤ 0.1 × tolerance",
          "unit": "measurement capability"
        }
